# HeyQ Environment Configuration
# Copy this to .env and add your API keys

# OpenAI Configuration (for GPT-4o-mini voice-to-intent parsing)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration (for Claude voice-to-intent parsing) 
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# HeyQ Configuration
HEYQ_USE_LLM=true
HEYQ_USE_REAL_MCP=false
HEYQ_LLM_PROVIDER=openai  # openai | anthropic | auto

# Development Settings
HEYQ_DEBUG=true
HEYQ_LOG_LEVEL=INFO
